<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.livae.ff.app">

	<permission
		android:name="com.livae.ff.dev.gcm.permission.C2D_MESSAGE"
		android:protectionLevel="signature"/>

	<uses-permission android:name="com.livae.ff.dev.gcm.permission.C2D_MESSAGE"/>

	<application>

		<provider
			android:authorities="com.livae.ff.app.dev.provider.ContactsProvider"
			android:exported="false"
			android:label="@string/provider_contacts"
			android:name="com.livae.ff.app.provider.ContactsProvider"
			android:syncable="true"/>
		<provider
			android:authorities="com.livae.ff.app.dev.provider.ConversationsProvider"
			android:exported="false"
			android:label="@string/provider_conversations"
			android:name="com.livae.ff.app.provider.ConversationsProvider"
			android:syncable="true"/>

		<receiver
			android:name="com.livae.ff.app.receiver.CloudMessagesReceiver"
			android:permission="com.google.android.c2dm.permission.SEND">
			<intent-filter>
				<action android:name="com.google.android.c2dm.intent.RECEIVE"/>
				<category android:name="com.livae.ff.dev.gcm"/>
			</intent-filter>
		</receiver>

	</application>

</manifest>
